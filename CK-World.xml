<?xml version="1.0" encoding="utf-8" ?>
<CK-World>

  <SharedHttpClient />
  <ArtifactCenter />
  <LocalFeedProvider />
  <CKEnvKeyVault />
  <NuGetClient />
  <NPMClient />
  <CKSetupClient />
  <NPMProjectCenter />
  <SolutionCentral />

  <ArtifactRepositories>

    <Repository Type="NuGetAzure"
                Organization="Signature-OpenSource"
                FeedName="Default"
                CheckName="NuGetAzure:Signature-OpenSource-Default"
                CheckSecretKeyName="AZURE_FEED_SIGNATURE_OPENSOURCE_PAT" />

    <Repository Type="NPMAzure"
                NPMScope="@signature"
                Organization="Signature-OpenSource"
                FeedName="Default"
                CheckName="NPMAzure:@signature->Signature-OpenSource-Default"
                CheckSecretKeyName="AZURE_FEED_SIGNATURE_OPENSOURCE_PAT" />

    <Repository Type="CKSetup"
                CheckName="CKSetup:Public"
                CheckSecretKeyName="CKSETUPREMOTESTORE_PUSH_API_KEY" />

  </ArtifactRepositories>
  
  <SolutionSettings>
    <NuGetSources>
      <add Name="Signature-OpenSource" Url="https://pkgs.dev.azure.com/Signature-OpenSource/_packaging/Default/nuget/v3/index.json" >
        <Credentials UserName="SignatureOpenSource" PasswordSecretKeyName="SIGNATURE_OPENSOURCE_READ_PAT" />
      </add>
    </NuGetSources>
    <NPMSources>
      <add Scope="@signature" Url="https://pkgs.dev.azure.com/Signature-OpenSource/_packaging/Default/npm/registry/" >
        <Credentials UserName="SignatureOpenSource" PasswordSecretKeyName="SIGNATURE_OPENSOURCE_READ_PAT" />
      </add>
    </NPMSources>
    <RemoveNuGetSourceNames>
      <add Name="CKEnvTest" />
      <add Name="CKEnvTest3" />
      <add Name="Invenietis - Release" />
      <add Name="Invenietis - Preview" />
      <add Name="Invenietis - CI" />
    </RemoveNuGetSourceNames>
    <ArtifactTargets>
      <add Name="NPMAzure:@signature->Signature-OpenSource-Default" />
      <add Name="NuGetAzure:Signature-OpenSource-Default" />
      <add Name="CKSetup:Public" />
    </ArtifactTargets>
    <Plugins>
      <add Type="CK.Env.Plugin.SolutionDriver, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.CodeCakeBuilderFolder, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.CodeCakeBuilderCSProjFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.CodeCakeBuilderKeyVaultFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.CodeCakeBuilderNPMSolutionFile, CK.Env.Plugin" />
      
      <add Type="CK.Env.Plugin.SolutionFiles.CommonFolder, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.SharedPropsFile, CK.Env.Plugin" />
      
      <add Type="CK.Env.Plugin.SolutionFiles.SolutionFolder, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.CKSetupStoreTestHelperConfigFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.RepositoryXmlFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.NugetConfigFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.GitIgnoreFile, CK.Env.Plugin" />
      <add Type="CK.Env.Plugin.SolutionFiles.NPMRCFiles, CK.Env.Plugin" />

      <add Type="CK.BuildSystem.Appveyor.AppveyorFile, CK.BuildSystem.Appveyor" />
      <add Type="CK.BuildSystem.GitLab.GitLabFile, CK.BuildSystem.GitLab" />
    </Plugins>
  </SolutionSettings>
  
